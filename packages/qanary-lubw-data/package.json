{
  "name": "qanary-lubw-data",
  "version": "1.0.0",
  "description": "Specialized LUBW data needed in Qanary",
  "license" : "SEE LICENSE IN LICENSE",
  "author": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --c src",
    "prettier:fix": "prettier --w src",
    "prepublishOnly": "npm run build",
    "generate:csv": "ts-node src/scripts/generate-complete-measurand-ner-csv-data.ts",
    "generate:json": "ts-node src/scripts/generate-complete-measurand-ner-json-data.ts",
    "generate:yml": "ts-node src/scripts/generate-complete-measurand-nlu-yml-data.ts",
    "generate:yml-small": "ts-node src/scripts/generate-complete-measurand-small-nlu-yml-data.ts",
    "generate:yml-measurand-max": "ts-node src/scripts/generate-measurand-max-nlu-yml-data.ts",
    "generate:yml-measurand-min": "ts-node src/scripts/generate-measurand-min-nlu-yml-data.ts",
    "generate:yml-measurand-threshold": "ts-node src/scripts/generate-measurand-threshold-nlu-yml-data.ts",
    "test": "jest --coverage --detectOpenHandles"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "date-fns": "2.29.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "eslint": "^8.33.0",
    "eslint-config-sdk": "*",
    "prettier": "^2.8.3",
    "prettier-config-sdk": "*",
    "ts-config-sdk": "*",
    "typescript": "^4.9.4",
    "jest": "^29.4.1",
    "jest-config-sdk": "*"
  },
  "jest": {
    "preset": "jest-config-sdk"
  },
  "prettier": "prettier-config-sdk",
  "volta": {
    "extends": "../../package.json"
  }
}
