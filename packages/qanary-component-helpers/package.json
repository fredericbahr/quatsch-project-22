    "api": "workspace:*",
    "eslint": "^8.33.0",
    "eslint-config-sdk": "workspace:*",
    "jest": "^29.4.1",
    "jest-config-sdk": "workspace:*",
    "openapi-types": "12.1.0",
    "prettier": "^2.8.3",
    "prettier-config-sdk": "workspace:*",
    "ts-config-sdk": "workspace:*",
    "typescript": "^4.9.5"
  },
  "jest": {
