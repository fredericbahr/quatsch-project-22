{
  "name": "qanary-component-ner-automl",
  "version": "1.0.0",
  "description": "A qanary NER automl component with training data generation",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "dist"
  ],
  "scripts": {
    "generate:csv": "ts-node src/generateNerCsvData.ts",
    "generate:json": "ts-node src/generateNerJsonData.ts",
    "generate:yml": "ts-node src/generateNluYmlData.ts",
    "build": "tsc",
    "clean": "rimraf .turbo coverage dist",
    "format": "prettier --check src",
    "format:fix": "prettier --write src",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest --coverage --detectOpenHandles"
  },
  "dependencies": {
    "qanary-lubw-data": "*",
    "shared": "*"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "eslint": "^8.33.0",
    "eslint-config-sdk": "*",
    "prettier": "^2.8.3",
    "prettier-config-sdk": "*",
    "rimraf": "^4.1.2",
    "ts-config-sdk": "*",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "jest": "^29.4.1",
    "jest-config-sdk": "*"
  },
  "jest": {
    "preset": "jest-config-sdk"
  },
  "prettier": "prettier-config-sdk",
  "volta": {
    "extends": "../../package.json"
  }
}
