{
  "name": "qanary-component-pm",
  "version": "1.0.0",
  "description": "A qanary component for a simple pattern matching based on a lubw domain",
  "license": "ISC",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "nodemon src/index.ts",
    "format": "prettier --check src",
    "format:fix": "prettier --write src",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest --coverage --detectOpenHandles"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "api": "workspace:*",
    "express": "^4.18.2",
    "qanary-component-core": "workspace:*",
    "qanary-component-helpers": "workspace:*",
    "qanary-lubw-data": "workspace:*",
    "qanary-seeding-helpers": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.11.18",
    "rdf-js": "^4.0.2",
    "@types/jest": "^29.4.0",
    "@types/mocha": "^10.0.1",
    "eslint": "^8.33.0",
    "eslint-config-sdk": "workspace:*",
    "jest": "^29.4.1",
    "jest-config-sdk": "workspace:*",
    "nodemon": "2.0.20",
    "openapi-types": "12.1.0",
    "prettier": "^2.8.3",
    "prettier-config-sdk": "workspace:*",
    "rimraf": "^4.1.2",
    "ts-config-sdk": "workspace:*",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "jest-config-sdk",
    "setupFiles": [
      "<rootDir>/setJestEnvVars.js"
    ]
  },
  "prettier": "prettier-config-sdk",
  "volta": {
    "extends": "../../package.json"
  }
}
